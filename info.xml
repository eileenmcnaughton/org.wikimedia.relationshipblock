<?xml version="1.0"?>
<extension key="org.wikimedia.relationshipblock" type="module">
  <file>relationshipblock</file>
  <name>Relationship Block</name>
  <description>Adds easy-edit relationship blocks to the contact summary page</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>emcnaughton@wikimedia.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org/extensions/relationship-block</url>
    <url desc="Documentation">https://github.com/eileenmcnaughton/org.wikimedia.relationshipblock</url>
    <url desc="Support">https://github.com/eileenmcnaughton/org.wikimedia.relationshipblock/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-11-03</releaseDate>
  <version>1.8</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.82</ver>
  </compatibility>
  <smarty_compatibility>
    <ver>5.0</ver>
  </smarty_compatibility>
  <php_compatibility>
    <ver>8.2</ver>
    <ver>8.3</ver>
    <ver>8.4</ver>
  </php_compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Relationshipblock</namespace>
    <format>25.01.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Relationshipblock_Upgrader</upgrader>
  <mixins>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
